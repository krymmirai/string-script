function plum()
    local args = {
        [1] = "Plum Purity"
    }
    
    game:GetService("ReplicatedStorage").CORE_RemoteEvents.SendSellRequest:FireServer(unpack(args))
end
function weak()
    local args = {
        [1] = "Weak Starpurge"
    }
    
    game:GetService("ReplicatedStorage").CORE_RemoteEvents.SendSellRequest:FireServer(unpack(args))
end
