local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Zero2 Hub", "DarkTheme")


local Tab = Window:NewTab("Misc")
local AutoFarm = Window:NewTab("Auto Farm")
local SellBoost = Window:NewTab("Sell Position")



local Misc = Tab:NewSection("Misc")
local Afarm = AutoFarm:NewSection("Farm")
local Sboost = SellBoost:NewSection("Sell")


--Misc
Misc:NewButton("Bypass Hold", "ButtonInfo", function()
    for i, v in pairs(workspace:GetDescendants()) do
        if v.ClassName == "ProximityPrompt" then
        v.HoldDuration = 0
        end
        end
end)

Misc:NewButton("Blacksmith", "ButtonInfo", function()
    local args = {
        [1] = "Blacksmith",
        [2] = 1,
        [3] = 1
    }
    
    game:GetService("ReplicatedStorage").CORE_RemoteEvents.SendDialogueRequest:FireServer(unpack(args))
    
end)

--Afarm

getgenv().name = true

Afarm:NewToggle("Auto Spin", "Spin", function(nameF)-- Auto Spin
    getgenv().name = nameF
    if nameF then
        dospin();
    end
end)

function dospin()
    spawn(function()
        while name == true do
            local args = {[1] = 1}
            game:GetService("ReplicatedStorage").CORE_RemoteEvents.SendSummonRequest:FireServer(5)
            wait(0.1)
        end
    end)
 end

 getgenv().farm = true

 Afarm:NewToggle("Auto Farm Item", "Item Farm", function(nameA)
     getgenv().farm = nameA
     if nameA then
         dofarm();
     end
 end)
 
 function dofarm()
     spawn(function()
         while farm == true do
             local args = {[1] = 1}
             Farmitem()
             wait(3)
         end
     end)
  end

  getgenv().delpoit = true

--Sboost

Sboost:NewToggle("Auto Sell Potions", "Potions Sell", function(nameB)
      getgenv().delpoit = nameB
      if nameB then
          dodel();
      end
  end)
  
  function dodel()
      spawn(function()
          while delpoit == true do
              local args = {[1] = 1}
              delboost()
              wait(0.3)
          end
      end)
   end
 




Misc:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.P, function()
	Library:ToggleUI()
end)


--new



function Farmitem()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-7.27, 74.72, -163.05)--caves 1
    wait(0.3)
    keypress(0x45)
    wait()
    keypress(0x45)
    wait(1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(289.66, 108.2, 97.45)--cave 2 point 1
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(326.37, 108.2, -33.68)--cave 2 point 2
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(363.01, 110.7, 120.09)
    wait(0.1)
    keypress(0x45)
    wait()
    keypress(0x45)
wait(3)
end
--Sboost Function
getgenv().plumpo = true
Sboost:NewToggle("Plum Purity", "Potions Sell", function(name1)
    getgenv().plumpo = name1
    if name1 then
        doplum();
    end
end)

function doplum()
    spawn(function()
        while plumpo == true do
            local args = {[1] = 1}
            plum()
            wait(0.3)
        end
    end)
 end


 getgenv().weakpo = true
Sboost:NewToggle("Weak Starpurge", "Potions Sell", function(name2)
    getgenv().weakpo = name2
    if name2 then
        doweak();
    end
end)

function doweak()
    spawn(function()
        while weakpo == true do
            local args = {[1] = 1}
            weak()
            wait(0.3)
        end
    end)
 end

