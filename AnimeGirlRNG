local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("TITLE", "DarkTheme")


local Tab = Window:NewTab("Misc")


local Misc = Tab:NewSection("Misc")


Misc:NewButton("Bypass Hold", "ButtonInfo", function()
    for i, v in pairs(workspace:GetDescendants()) do
        if v.ClassName == "ProximityPrompt" then
        v.HoldDuration = 0
        end
        end
end)

getgenv().name = true

Misc:NewToggle("Auto Spin", "Spin", function(nameF)-- Auto Spin
    getgenv().name = nameF
    if nameF then
        dospin();
    end
end)

function dospin()
    spawn(function()
        while name == true do
            local args = {[1] = 1}
            game:GetService("ReplicatedStorage").CORE_RemoteEvents.SendSummonRequest:FireServer(5)
            wait(0.3)
        end
    end)
 end

 getgenv().farm = true

 Misc:NewToggle("Auto Farm Item", "Item Farm", function(nameA)
     getgenv().farm = nameA
     if nameA then
         dofarm();
     end
 end)
 
 function dofarm()
     spawn(function()
         while farm == true do
             local args = {[1] = 1}
             Farmitem()
             wait(19)
         end
     end)
  end

  getgenv().delpoit = true

  Misc:NewToggle("Auto Farm Item", "Item Farm", function(nameB)
      getgenv().delpoit = nameB
      if nameB then
          dodel();
      end
  end)
  
  function dodel()
      spawn(function()
          while delpoit == true do
              local args = {[1] = 1}
              --test
              wait(3)
          end
      end)
   end
 




Misc:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.P, function()
	Library:ToggleUI()
end)






function Farmitem()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-7.27, 74.72, -163.05)--caves 1
    wait(0.3)
    keypress(0x45)
    wait()
    keypress(0x45)
    wait(1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(289.66, 108.2, 97.45)--cave 2 point 1
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(326.37, 108.2, -33.68)--cave 2 point 2
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(363.01, 110.7, 120.09)
    wait(0.1)
    keypress(0x45)
    wait()
    keypress(0x45)
end
    
